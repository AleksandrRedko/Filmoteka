{{#each this}}
{{#if dots}} <li class="pagination__item pagination__item--dots" >{{page}}</li>
{{else if current}} 
<li class="pagination__item pagination__item--current btn"  >{{page}}</li>
{{else}} 
<li class="pagination__item" ><button type="button" class="pagination__btn pagination__btn--page btn">{{page}}</button></li> 
{{/if}}
{{/each}}